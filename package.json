{
    "name": "raschka",
    "version": "1.0.0",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "build:css": "tailwindcss -i ./public/css/style.css -o ./public/style.css",
      "build": "npm run build:css",
      "dev": "concurrently \"npm run start\" \"npm run build:css --watch\"",
      "watch:dev": "concurrently \"nodemon server.js\" \"npm run build:css --watch\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "Convert EPS files to SVG using Node.js and Ghostscript",
    "dependencies": {
      "cors": "^2.8.5",
      "express": "^4.21.1",
      "multer": "^1.4.5-lts.1",
      "path": "^0.12.7"
    },
    "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^5.0.0",
      "@types/multer": "^1.4.12",
      "autoprefixer": "^10.4.20",
      "concurrently": "^9.0.1",
      "postcss": "^8.4.47",
      "tailwindcss": "^3.4.13"
    }
  }
  